<%= form_with(model: @room, local: true) do |f| %>
<% if @room.errors.any? %>
  <div id="error_explanation">
    <ul>
      <% @room.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>

  <%= f.label :image, '画像' %>
  <%= f.file_field :image %>
  
  <%= f.label :content, '物件名' %>
  <%= f.text_field :content %>
  
  <%= f.label :area_id, '地域ナンバー' %>
  <%= f.text_field :area_id %>
  
  <%= f.label :rent, '賃料' %>
  <%= f.text_field :rent %>
  
  <%= f.label :management, '管理費' %>
  <%= f.text_field :management %>
  
  <%= f.label :floor, '間取り' %>
  <%= f.text_field :floor %>
  
  <%= f.label :age, '築年数' %>
  <%= f.text_field :age %>
  
  <%= f.label :number_of_floor, '階数' %>
  <%= f.text_field :number_of_floor %>
  
  <%= f.label :street_address, '住所' %>
  <%= f.text_field :street_address %>
  
  <%= f.submit '投稿' %>
<% end %>
